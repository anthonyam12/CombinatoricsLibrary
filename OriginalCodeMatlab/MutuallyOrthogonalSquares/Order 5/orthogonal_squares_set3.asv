%This script finds sets of 3 squares that are mutually othogonal
%It will only use the squares that belonged to a set of two. 

fid = fopen('mutually_orthogonal_squaresO5.dat');

pairs = zeros(1,2, 0);

while ~feof(fid)
   a = fscanf(fid, '%u %u',[1 2]);
   pairs = cat(3, pairs, a);
end

fclose(fid);

